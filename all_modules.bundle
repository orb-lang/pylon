binary = "\n\n\n\n\n\nlocal Set = {}\nSet.__index = Set\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nlocal _s = setmetatable({}, { __mode = \'k\' })\n\n\n\n\n\n\n\n\n\n\n\n\n\nlocal function new(items)\n   -- set /object/\n   local set = {}\n   -- set /collection/\n   local âˆˆ = {}\n   -- cached in the attribute table as per usual\n   _s[set] = âˆˆ\n   setmetatable(set, Set)\n\n   assert(type(items) == \"table\" or items == nil,\n          \"argument to set constructor must be a table\")\n   if items then\n      if items.idEst == new then\n         for k, _ in pairs(_s[items]) do\n            âˆˆ[k] = true\n         end\n      else\n         for _, value in ipairs(items) do\n            âˆˆ[value] = true\n         end\n      end\n   end\n   return set\nend\nSet.idEst = new\n\n\n\n\n\n\n\n\n\n\n\nfunction Set.insert(set, ...)\n   local âˆˆ = assert(_s[set], \"set elements lost at C\")\n   local items = pack(...)\n   for i = 1, items.n do\n      if items[i] then\n         âˆˆ[items[i]] = true\n      end\n   end\nend\n\n\n\n\n\n\n\n\n\n\n\n\nfunction Set.remove(set, ...)\n   local âˆˆ = assert(_s[set], \"set elements lost at C\")\n   local removed = true\n   local items = pack(...)\n   for i = 1, items.n do\n      removed = âˆˆ[items[i]] and removed\n      âˆˆ[items[i]] = nil\n   end\n   return removed and removed or nil\nend\n\n\n\n\n\n\n\n\nlocal clear = assert(table.clear)\n\nfunction Set.clear(set)\n   local âˆˆ = assert(_s[set], \"set elements lost at C\")\n   clear(âˆˆ)\nend\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\nfunction Set.âˆˆ(set, item)\n   local âˆˆ = assert(_s[set], \"set elements lost at C\")\n   return âˆˆ[item] or false\nend\n\nSet.E = Set.âˆˆ\n\n\n\n\n\n\n\n\nfunction Set.__call(set, item)\n   local âˆˆ = assert(_s[set], \"set elements lost at C\")\n   return âˆˆ[item] or false\nend\n\n\n\n\n\n\nlocal function hasSubset(set, maybe_subset)\n   assert(type(maybe_subset) == \"table\", \"bad argument #2 to hasSubset\")\n   if maybe_subset.idEst ~= new then