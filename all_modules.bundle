binary = "\nlocal core = require \"qor:core\"\n\n\n\n\nlocal nums = {\n   [0] = [[\nâ”â”â”“\nâ”ƒÂ·â”ƒ\nâ”—â”â”›]],\n[[\n â”“\n â”ƒ\n â”»]],\n[[\nâ•ºâ”â”“\nâ”â”â”›\nâ”—â”â•¸]],\n[[\nâ•ºâ”â”“\nâ•ºâ”â”«\nâ•ºâ”â”›]],\n[[\nâ•» â•»\nâ”—â”â”«\n  â•¹]],\n[[\nâ”â”â•¸\nâ”—â”â”“\nâ•ºâ”â”›]],\n[[\nâ”â”â•¸\nâ” â”â”“\nâ”—â”â”›]],\n[[\nâ•ºâ”â”“\n  â•‹\n  â•¹]],\n[[\nâ”â”â”“\nâ” â”â”«\nâ”—â”â”›]],\n[[\nâ”â”â”“\nâ”—â”â”«\nâ•ºâ”â”›]], -- but wait! there\'s more!\n\"   \\nâ”â•¸\\n   \",\n\"   \\n ï½¡ \\n   \",\n}\n\n\n\n\n\nlocal sub = string.sub\n\nlocal lines = core.string.lines\n\nlocal hi, mid, low = {}, {}, {}\n\nfor i = 0, 11 do\n   local is_one = i == 1\n   local _num = nums[i]\n   local st = \'hi\'\n   for stripe in lines(_num) do\n      if is_one then\n         stripe = stripe .. \" \"\n      end\n      if st == \'hi\' then\n         hi[i] = stripe\n         st = \'mid\'\n      elseif st == \'mid\' then\n         mid[i] = stripe\n         st = \'low\'\n      elseif st == \'low\' then\n         low[i] = stripe\n      end\n   end\nend\n\n\n\n\n\nlocal insert, concat = assert(table.insert), assert(table.concat)\n\nreturn function(num)\n   local numstr = tostring(num)\n   if #numstr == 1 then\n      return nums[num]\n   end\n\n   local _hi, _mid, _low = {}, {}, {}\n   for digit in numstr:gmatch \'.\' do\n      local n = tonumber(digit)\n      insert(_hi, hi[n])\n      insert(_mid, mid[n])\n      insert(_low, low[n])\n   end\n   return concat(_hi)\n          .. \"\\n\" .. concat(_mid)\n          .. \"\\n\" .. concat(_low)\nend\n\n",
