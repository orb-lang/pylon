binary = "\n\n\n\n\n\n\n\n\n\n\n\n\n\nlocal lua_parser = require \"helm/lua-parser\"\nlocal names = require \"repr:repr/names\"\n\nlocal meta = require \"core:core/meta\" . meta\nlocal fn = require \"core:core/fn\"\nlocal Tab = require \"core:core/table\"\n\n\n\n\n\n\n\n\n\n\n\n\nlocal req = assert(require)\nlocal keys = require \"core:table\" . keys\nlocal function requirer(req_tab, module)\n   if not req_tab.restarting then\n      req_tab.mods[module] = true\n      return req(module)\n   else\n      if req_tab.cleared[module] then\n         return req(module)\n      end\n      local hash = _Bridge.load_hashes[module]\n      if not hash then\n         return req(module)\n      else\n         -- invalidate our various caches\n         package.loaded[module] = nil\n         _Bridge.loaded[hash] = nil\n         _Bridge.load_hashes[module] = nil\n         -- mark as cleared so we don\'t re-invalidate it\n         req_tab.cleared[module] = true\n         return req(module)\n      end\n   end\nend\n\nlocal function _reset(req_tab)\n   req_tab.cleared = {}\n   req_tab.restarting = nil\nend\n\nlocal req_M = { __call = requirer, __index = { reset = _reset } }\n\nlocal function requisition()\n   return setmetatable({ mods = {}, cleared = {} }, req_M)\nend\n\n\n\n\n\n\n\n\n\n\n\n\n\nlocal function indexer(Env, key)\n   return Env[key]\nend\n\nlocal function make__index(env, env_2)\n   return function (eval_ENV, key)\n      local ok, value = pcall(indexer, env, key)\n      if ok and value ~= nil then\n         return value\n      end\n      if env_2 then\n         ok, value = pcall(indexer, env_2, key)\n         if ok and value ~= nil then\n            return value\n         end\n      end\n      return nil\n   end\nend\n\nlocal function newindexer(Env, key, value)\n   Env[key] = value\nend\n\n\n\nlocal loadNames = assert(names.loadNames)\n\nlocal function make__newindex(env, anti_G)\n   return function (eval_ENV, key, value)\n      local ok = pcall(newindexer, env, key, value)\n      if not ok then\n         rawset(env, key, value)\n      end\n      -- Use loadNames() to get the key added to all_symbols\n      -- Should really divide up responsibility better between\n      -- loadNames() and addName()\n      loadNames({ [key] = value }, nil, anti_G)\n   end\nend\n\n\n\nlocal function gatherResults(success, ...)\n  return success, pack(...)\nend\n\n\n\nlocal result_repr_M = meta {}\n\nfunction result_repr_M.__repr(result)\n  local i = 1\n  return function()\n     if i <= #result then\n       i = i + 1\n       return result[i - 1]\n     end\n  end\nend\n\n\n\n\n\n\n\n\n\n\n\nlocal insert = assert(table.insert)\nlocal keys = assert(Tab.keys)\nlocal sub = assert(string.sub)\n\nlocal function evaluator(eval_ENV, chunk)\n   -- check for leading =, old-school style\n   local head = sub(chunk, 1, 1)\n   if head == \"=\" then -- take pity on old-school Lua hackers\n       chunk = \"return \" .. sub(chunk,2)\n   end\n   -- add \"return\" and see if it parses\n   local return_chunk = \"return \" .. chunk\n   local parsed_chunk = lua_parser(return_chunk)\n   if not parsed_chunk:select \"Error\" () then\n      chunk = return_chunk\n   else\n      -- re-parse the chunk\n      parsed_chunk = lua_parser(chunk)\n   end\n   -- #Todo tinker with the chunk, finding $1-type vars\n   if parsed_chunk:select \"Error\" () then\n      -- our parser isn\'t perfect, let\'s see what lua thinks\n      local is_expr = loadstring(return_chunk)\n      if is_expr then\n         -- we have an expression which needs a return, and didn\'t\n         -- detect it:\n         chunk = return_chunk\n         -- otherwise, we\'ll try our luck with the chunk, as-is\n      end\n   end\n   local success, results\n   local f, err = loadstring(chunk, \'REPL\')\n   if f then\n      setfenv(f, eval_ENV)\n      success, results = gatherResults(xpcall(f, debug.traceback))\n      if not success then\n         -- error\n         results.frozen = true\n         return false, results\n      end\n   else\n      if err:match \"\'<eof>\'$\" then\n         -- Lua expects some more input\n         return false, \'advance\'\n      else\n         -- make the error into the result\n         results = { err,\n                     n = 1,\n                     frozen = true }\n         return false, results\n      end\n   end\n\n   return true, results\nend\n\n\n\n\n\n\n\n\n\nlocal partial = assert(fn.partial)\nlocal function new(env, env_2, anti_G)\n   assert(env, \"must provide at least one environment\")\n   anti_G = anti_G or assert(names.anti_G)\n   local eval_M = { __index = make__index(env, env_2),\n                    __newindex = make__newindex(env, anti_G)}\n   local eval_ENV = setmetatable({}, eval_M)\n   eval_ENV.require = requisition()\n   return partial(evaluator, eval_ENV), eval_ENV.require\nend\n\n\n\nreturn new\n\n",
