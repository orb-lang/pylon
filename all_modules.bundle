ineUsing(phrase, box.bl_square, box.bottom_tee, box.br_square, box.horiz, ...)\nend\n\n\n\n\n\n\n\n\n\nfunction Box.__call(box, rect, ...)\n   if not instanceof(rect, Rectangle) then\n      rect = Rectange(rect, ...)\n   end\n   local width = rect:width()\n   local phrase = {}\n   insert(phrase, jump(rect:origin()))\n   box:addTopLine(phrase, width - 2)\n   local backup  = jump.back(width) .. jump.down(1)\n   local forward = jump.forward(width - 2)\n   for j = 1, rect:height() - 2 do\n      -- jump back and forth\n      insert(phrase, backup)\n      insert(phrase, box.vert) -- ‚îÇ\n      insert(phrase, forward)\n      insert(phrase, box.vert)\n   end\n   insert(phrase,backup)\n   box:addBottomLine(phrase, width - 2)\n   return concat(phrase)\nend\n\n\n\n\n\n\n\n\nlocal box = {}\nfor box_type, slug in pairs(boxen) do\n   local this_box = meta(Box)\n   for i = 1, #box_parts do\n      this_box[box_parts[i]] = slug[i]\n   end\n   box[box_type] = this_box\nend\n\n\n\nreturn box\n\n",
